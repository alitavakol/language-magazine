<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/lock_fill"
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="oval">
            <solid android:color="#88000000" />
        </shape>
    </item>
    <item android:id="@+id/lock_border">
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="#88000000" />
        </shape>
    </item>
</layer-list>